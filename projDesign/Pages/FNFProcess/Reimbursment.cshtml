<div class="mt-4">
    <div class="col-md-12">
        <div class="col-md-6">
            <div class="col-md-4"><label class="col-form-label">Reimbursment Head <i>*</i></label></div>
            <div class="col-md-8">
                <input type="text" id="txtreimhead" placeholder="Reimbursment Head" onkeypress="return AlphaNumeric(event);" maxlength="100" class="form-control" />
            </div>
        </div>
        <div class="col-md-6">
            <div class="col-md-4"><label class="col-form-label">Amount <i>*</i></label></div>
            <div class="col-md-8">
                <input type="text" id="txtreimamt" placeholder="Amount" onkeypress="return isNumber(event,this);" maxlength="10" class="form-control" />
            </div>
        </div>
    </div>

    <div class="clear"></div>
    <div class="col-md-12">
        <div class="form-group text-left">
            <button type="button" class="btn btn-primary btn-lg" id="btnreim_save">Save</button>
        </div>
    </div>

    <div class="col-md-12">

        <hr />
        <div class="table-responsive">
            <table id="tbl_reim_dtl" class="table nowrap w-100 no-margin"></table>
            <div class="clear"></div>
        </div>
    </div>

</div> 

<div class="clearfix"></div>

