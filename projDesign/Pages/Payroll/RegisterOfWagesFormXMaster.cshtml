        <div class="col-md-6">
            <div class="col-md-6 text-rigth"><label>Signature or Thumb-Impression of Employee</label></div>
            <div class="col-md-6">
                <input type="file" id="File1" />
            </div>
        </div>*@

                    <div class="col-md-12">
                     
                        <div class="form-group text-center">
                            <button type="button" class="btn btn-primary btn-lg" id="btnsave">Save</button>
                            <button type="button" id="btnreset" class="btn btn-primary btn-lg">Reset</button>
                        </div>
                 
                    </div>
                    <div class="clear"></div>
                </div>


            </div>
        </div>
    </div>
</div>


<script src="~/apiJS/Payroll/JsRegisterofWagesFormXMaster.js?ver=@DateTime.Now.Ticks.ToString()"></script>